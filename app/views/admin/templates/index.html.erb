<h1>测试项目</h1>

<hr>

<table>
  <% @temp_names.each_with_index do |temp_name, index| %>
    <tr>
      <td><%= index +1 %></td>
      <td><%= temp_name.name %></td>
    </tr>
    <tr>
      <td> &nbsp;</td>
      <td>
        <% temp_name.templates.each do |template| %>
          <%= @item[template.item_id] %>
        <% end %>
      </td>
    </tr>

  <% end %>

</table>
